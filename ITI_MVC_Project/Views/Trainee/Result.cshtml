@model TraineeResultViewModel
@{
    ViewData["Title"] = "Result";
}


@section asideNav{}

<div class="card shadow p-3" >
    <div class="card-header text-white bg-dark">
        <h3 class="card-title text-light text-bolder">Trainee Result</h3>
    </div>
    <div class="card-body p-3">
        <h4>Trainee Name: @Model.TraineeName </h4>
            <h5>Course Name: @Model.CourseName</h5>
            <h5>Degree: @Model.TraineeDegree/@Model.TotalDegree</h5>
            <h5 style="color:@Model.Color">Status: @Model.Status</h5>
    </div>
</div>